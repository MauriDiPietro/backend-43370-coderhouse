<div>
{{#if users.length}}
    {{#each users}}
        <ul>
            <li>{{firstname}}</li>
            <li>{{lastname}}</li>
            <li>{{age}}</li>
            <li>{{mail}}</li>
        </ul>
    {{/each}}
    {{else}}
        <p>No hay usuarios</p>
{{/if}}
<script src="/js/index.js"></script>
</div>